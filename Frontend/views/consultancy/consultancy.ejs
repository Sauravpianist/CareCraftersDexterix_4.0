<body>
    <%- include("../partials/header.ejs") %>
    <div class="dropdown">
      <h2>Find doctors</h2>
        <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
          Find cities
        </button>
        <ul class="dropdown-menu">

          <li><a class="dropdown-item" href="#">Lucknow</a></li>
          <li><a class="dropdown-item" href="#">Mumbai</a></li>
          <li><a class="dropdown-item" href="#">Delhi</a></li>
        </ul>
        <br>
        <br>
        <label for="username">Preferred Location/Pincode*:</label>
        <input type="text" id="username" name="username" placeholder="Enter your location">
        <hr>
        <label for="username">Select Date*:</label>
        <input type="text" id="username" name="username" placeholder="Enter date">

        <hr>
        <label for="username">Select Speciality*:</label>
        <input type="text" id="username" name="username" placeholder="Enter Speciality">

        <hr>
        <button type="submit">Submit</button>
      </div>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
